	"type": "module",
	"homepage": "https://github.com/lfabbriend/mealsavior#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lfabbriend/mealsavior.git"
	},
	"bugs": {
		"url": "https://github.com/lfabbriend/mealsavior/issues"
	},
	"scripts": {
		"prettier": "prettier --write **/*.{js,scss,json,html}",
		"sass": "sass public/scss/main.scss public/css/style.css --style=compressed --no-source-map",
		"sass:watch": "sass public/scss/main.scss public/css/style.css --style=compressed --watch",
		"dev:server": "nodemon index.js",
		"predev": "npm i",
		"dev": "npm-run-all --parallel dev:server sass:watch",
		"start:server": "node index.js",
		"prestart": "npm i",
		"start": "npm-run-all --parallel start:server sass",
		"test:ui": "npx cypress run",
		"test:api": "npx newman run test/api/mealsavior.api"
	},
	"devDependencies": {
		"cypress": "^9.6.1",
		"dotenv": "^16.0.1",
		"newman": "^5.3.2",
		"nodemon": "^2.0.16",
		"prettier": "^2.6.2",
		"sass": "^1.50.1"
	},
	"dependencies": {
		"bcrypt": "^5.0.1",
		"cors": "^2.8.5",
		"cypress-cucumber-preprocessor": "^4.3.1",
		"express": "^4.18.1",
		"handlebars": "^4.7.7",
		"hbs": "^4.2.0",
		"jsonwebtoken": "^8.5.1",
		"live-server": "^1.1.0",
		"mongoose": "^6.3.4",
		"npm-run-all": "^4.1.5"
	},
	"cypress-cucumber-preprocessor": {
		"nonGlobalStepDefinitions": true
	}
}
