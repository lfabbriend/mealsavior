{
    "name": "example",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "node": "node index.js",
        "nodemon": "nodemon index.js",
        "sass:watch": "sass ./public/scss/main.scss ./public/css/style.css --watch --style=compressed",
        "sass": "sass ./public/scss/main.scss ./public/css/style.css --style=compressed --no-source-map",
        "start": "npm-run-all --parallel node sass",
        "dev": "npm-run-all --parallel nodemon sass:watch"
    },
    "dependencies": {
        "express": "^4.17.3",
        "hbs": "^4.2.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "sass": "^1.50.1"
    }
}
